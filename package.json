{
  "name": "shortstat",
  "description": "`git log --shortstat`, but shorter.",
  "version": "0.1.1",
  "author": "Mark Tiedemann <www.marktiedemann@gmail.com>",
  "license": "WTFPL",
  "bin": {
    "shortstat": "dist/bin.js"
  },
  "scripts": {
    "format": "prettier --write \"**/*.{ts,json,md}\"",
